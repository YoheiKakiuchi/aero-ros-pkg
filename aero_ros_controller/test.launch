<launch>
  <!--include file="" >
      </include-->
  <node pkg="aero_startup" type="aero_controller_wrapper"
        name="aero_raw_controller"
        output="screen" >
  </node>
  <node pkg="aero_ros_controller" type="aero_ros_controller"
        name="ros_joint_controller_mamager"
        output="screen" >
  </node>
  <rosparam>
    joint_state_controller:
      type: joint_state_controller/JointStateController
      publish_rate: 50
    rarm_controller:
      type: position_controllers/JointTrajectoryController
      joints:
        - r_shoulder_p_joint
        - r_shoulder_r_joint
        - r_shoulder_y_joint
        - r_elbow_joint
        - r_wrist_y_joint
        - r_wrist_p_joint
        - r_wrist_r_joint
#        - r_hand_y_joint
    larm_controller:
      type: position_controllers/JointTrajectoryController
      joints:
        - l_shoulder_p_joint
        - l_shoulder_r_joint
        - l_shoulder_y_joint
        - l_elbow_joint
        - l_wrist_y_joint
        - l_wrist_p_joint
        - l_wrist_r_joint
#        - l_hand_y_joint
    head_controller:
      type: position_controllers/JointTrajectoryController
      joints:
        - neck_y_joint
        - neck_p_joint
        - neck_r_joint
    waist_controller:
      type: position_controllers/JointTrajectoryController
      joints:
        - waist_y_joint
        - waist_p_joint
        - waist_r_joint
    lift_controller:
      type: position_controllers/JointTrajectoryController
      joints:
        - ankle_joint
        - knee_joint
  </rosparam>
  <rosparam>
  aero_joint_names:
    - waist_y_joint
    - waist_p_joint
    - waist_r_joint
    - l_shoulder_p_joint
    - l_shoulder_r_joint
    - l_shoulder_y_joint
    - l_elbow_joint
    - l_wrist_y_joint
    - l_wrist_p_joint
    - l_wrist_r_joint
#    - l_hand_y_joint
    - l_indexbase_joint
    - l_indexmid_joint
    - l_indexend_joint
    - l_thumb_joint
    - neck_y_joint
    - neck_p_joint
    - neck_r_joint
    - r_shoulder_p_joint
    - r_shoulder_r_joint
    - r_shoulder_y_joint
    - r_elbow_joint
    - r_wrist_y_joint
    - r_wrist_p_joint
    - r_wrist_r_joint
#    - r_hand_y_joint
    - r_indexbase_joint
    - r_indexmid_joint
    - r_indexend_joint
    - r_thumb_joint
    - knee_joint_mimic
    - knee_joint
    - ankle_joint
  </rosparam>
  <pparam name="robot_description"
         command="$(find xacro)/xacro --inorder '$(find aero_description)/robots/aero.urdf.xacro'" />
  <pparam name="robot_description_semantic"
         textfile="$(find aero_moveit_config_new)/config/AeroUpperRobot.srdf" />
  <include file="$(find aero_moveit_config_new)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>

  <node name="aero_state_publisher"
       pkg="robot_state_publisher" type="robot_state_publisher" />

  <node name="cm_spawner" pkg="controller_manager"
        type="spawner" args="joint_state_controller larm_controller rarm_controller head_controller waist_controller lift_controller" />

</launch>
